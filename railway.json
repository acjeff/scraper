{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "python3 parallel_scraper.py",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 5
  },
  "services": [
    {
      "name": "scraper",
      "startCommand": "python3 parallel_scraper.py"
    },
    {
      "name": "resume",
      "startCommand": "python3 resume_scraper.py"
    }
  ]
}
